
class Parent {

    public  void m1()

    {

        System.out.println("do you like tea");

    }

  

    public   void m2()

    {

        System.out.println("I like chocolates ");

    }

}

  

class Child extends Parent {

 
 public    void m1()

    {

        System.out.println("No! I like cofee");

    }

    @Override

    public void m2()

    {

        System.out.println("i like ice-cream");

    }

}

class Main {

    public static void main(String[] args)

    {

        Parent obj1 = new Child();

  

        obj1.m1();

  

        obj1.m2();

    }
